
<nav id="page-navbar" class="navbar fixed-top bg-body navbar-expand border-bottom px-2" aria-label="{{#str}}sitemenubar, admin{{/str}}">
    <button class="navbar-toggler aabtn d-block d-md-none px-1 my-1 border-0" data-toggler="drawers" data-action="toggle" data-target="theme_boost-drawers-primary">
        <span class="navbar-toggler-icon"></span>
        <span class="sr-only">{{#str}}sidepanel, core{{/str}}</span>
    </button>
     <a href="{{{ config.homeurl }}}" class="navbar-brand d-none d-md-flex align-items-center m-0 me-4 p-0 aabtn">

        {{# output.get_custom_logo_url }}
            <img src="{{{ output.get_custom_logo_url }}}" class="logo me-1" alt="{{sitename}}">
        {{/ output.get_custom_logo_url }}
        {{^ output.get_custom_logo_url }}
            {{{ sitename }}}
        {{/ output.get_custom_logo_url }}
    </a>

    {{#primarymoremenu}}
        <div class="primary-navigation">
            {{> core/moremenu}}
        </div>
    {{/primarymoremenu}}

    <ul class="navbar-nav d-none d-md-flex my-1 px-1">
        {{{ output.page_heading_menu }}}
    </ul>

    <div id="usernavigation" class="navbar-nav ms-auto h-100">
        {{# output.search_box }}
            {{{ output.search_box }}}
            <div class="divider border-start h-75 align-self-center mx-1"></div>
        {{/output.search_box}}
        {{#langmenu}}
            {{> theme_boost/language_menu }}
            <div class="divider border-start h-75 align-self-center mx-1"></div>
        {{/langmenu}}
        {{{ output.navbar_plugin_output }}}
        <div class="d-flex align-items-stretch usermenu-container" data-region="usermenu">
            {{#usermenu}}
                {{> core/user_menu }}
            {{/usermenu}}
            {{#userregister}} <!-- Only show if the setting is enabled -->
                <div class="usermenu">
                    <span class="login pl-2">
                        <a href="{{userregisterurl}}">Register</a>
                    </span>
                </div>
            {{/userregister}}
        </div>
        {{{ output.edit_switch }}}
    </div>
</nav>

{{#secondarymoremenu}}
    <div id="seconndarymenu" class="secondary-navigation fixed-top d-print-none ap">
        <div class="containerx">
            {{> core/moremenu}}
        </div>
    </div>
{{/secondarymoremenu}}

